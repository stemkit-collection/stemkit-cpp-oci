# vim: ft=jam: sw=2:

local library = sk.oci ;

local objects = 
  Exception.cc
  NoDataException.cc
  ErrorException.cc
  InvalidHandleException.cc
  StillExecutingException.cc
  ContinueException.cc
;

make-library $(library) : $(objects) ;

add-project-library $(library) : 
  $(library).info
  $(library).abstract
;

make-subdirs info abstract db mock tests ;
