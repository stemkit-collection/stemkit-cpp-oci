# vim: ft=jam: sw=2:

local programs = 
  oracle-probe 
  describe-table 
  select-all
  minimal-select-all
  select-some
;

use-project-libraries $(programs) : sk.oci sk.oci.db ;

local program ; 
for program in $(programs) {
  make-program $(program) : $(program).cc ;
}

make-subdirs functional ;
